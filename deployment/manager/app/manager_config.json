{
  "loadLocales" : true,
  "languages" : {
    "en" : "english",
    "de" : "german"
  },
  "realms" : {
    "default" : {
      "appTitle" : "K2 IoT Platform",
      "styles" : ":host > * {--or-app-color2: #F9F9F9; --or-app-color3: #22211f; --or-app-color4: #e3000a; --or-app-color5: #CCCCCC;}",
      "logo" : "/images/default/logo.png",
      "logoMobile" : "/images/default/logoMobile.png",
      "favicon" : "/images/default/favicon.",
      "language" : "en"
    }
  },
  "pages" : {
    "rules" : {
      "rules" : {
        "descriptors" : {
          "all" : {
            "includeAssets" : [ "K2SnowGuardAsset", "ThingAsset", "GroupAsset", "HTTPAgent" ]
          }
        }
      }
    },
    "map" : {
      "markers" : {
        "K2SnowGuardAsset" : {
          "attributeName" : "weightPercentage",
          "showLabel" : true,
          "showUnits" : true,
          "colours" : {
            "type" : "range",
            "ranges" : [ {
              "max" : 50,
              "colour" : "39B54A"
            }, {
              "max" : 80,
              "colour" : "F7931E"
            }, {
              "max" : 100,
              "colour" : "C1272D"
            } ]
          }
        }
      },
      "card" : {
        "assetTypes" : {
          "K2SnowGuardAsset" : {
            "include" : [ "weightPercentage", "weightMax", "weightMeasuredTotal" ]
          }
        }
      }
    },
    "assets" : {
      "tree" : {
        "add" : {
          "typesParent" : {
            "default" : {
              "include" : [ "K2SnowGuardAsset", "ThingAsset", "GroupAsset", "HTTPAgent" ]
            }
          }
        }
      },
      "viewer" : {
        "assetTypes" : {
          "K2SnowGuardAsset" : {
            "viewerStyles" : { },
            "panels" : [ {
              "type" : "group",
              "title" : "underlyingAssets"
            }, {
              "type" : "info",
              "hideOnMobile" : true,
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "notes", "manufacturer", "model" ]
              }
            }, {
              "type" : "info",
              "column" : 1,
              "title" : "location",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "location" ],
                "itemConfig" : {
                  "location" : {
                    "label" : ""
                  }
                }
              }
            }, {
              "type" : "info",
              "title" : "Processed data",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "weightPercentage", "weightMax" ]
              }
            }, {
              "type" : "info",
              "title" : "Sensor data",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "temperatureInternal", "weightMeasured1", "weightMeasured2", "rawData1", "rawData2", "weightMeasuredTotal" ]
              }
            }, {
              "type" : "info",
              "title" : "Required info",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "loadMax", "pvSizeLength", "pvSizeWidth", "rawDataTare", "roofPitch" ]
              }
            }, {
              "type" : "info",
              "column" : 1,
              "title" : "Device info",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "deviceID", "factor", "firmwareVersionBaseBox", "hardwareVersionBaseBox", "firmwareVersionTopBox", "hardwareVersionTopBox", "serialNumberTopBox", "status", "errorIdentifier", "debugMessage" ]
              }
            }, {
              "type" : "info",
              "title" : "Optional info",
              "properties" : {
                "include" : [ ]
              },
              "attributes" : {
                "include" : [ "baseProject", "roofType", "roofCover", "k2Mounting", "k2Fasteners", "pvBrand", "pvModel" ]
              }
            }, {
              "type" : "history",
              "column" : 1
            }, {
              "type" : "linkedUsers",
              "column" : 1
            } ]
          }
        }
      }
    }
  }
}