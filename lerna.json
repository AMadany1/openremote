{
  "version": "1.0.4",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "npmClient": "npm",
      "ignoreChanges": [
        "*.js",
        "*.md"
      ],
      "message": "UI updates: Set version for publishing"
    }
  },
  "packages": [
    "ui/component/*",
    "ui/app/*"
  ]
}
